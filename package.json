{
  "name": "nest-boilerplate",
  "version": "0.0.1",
  "description": "API  for Basic",
  "author": "KuiperZ",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "export NODE_ENV=production && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "export NODE_ENV=development && export PORT=9000 && nest start --watch",
    "start:win": "set NODE_ENV=development && set PORT=9000 && nest start --watch",
    "start:dev": "export NODE_ENV=development && export PORT=9000 && nest start --watch",
    "start:staging": "export NODE_ENV=staging && export PORT=9000 && nest start --watch",
    "start:dev:win": "nest start:win --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^1.0.1",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "^9.3.11",
    "@nestjs/core": "^9.3.11",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.11",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "aws-sdk": "^2.1340.0",
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "dd-trace": "^3.15.0",
    "dotenv": "^16.0.3",
    "express-basic-auth": "^1.2.1",
    "handlebars": "^4.7.7",
    "ioredis": "^5.3.1",
    "jsonwebtoken": "^9.0.0",
    "nest-winston": "^1.9.1",
    "nestjs-stripe": "^1.0.0",
    "passport": "^0.6.0",
    "path": "^0.12.7",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "stripe": "^11.15.0",
    "typeorm": "^0.3.12",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-scope": "^0.1.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.3.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.11",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.5",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "prettier": "^2.8.6",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "^5.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
